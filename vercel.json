
{
    "version": 2,
    "builds": [
      { "src": "public/bundle.js", "use": "@vercel/static" },
      { "src": "public/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/$1",
        "headers": { "Cache-Control": "public, max-age=0, must-revalidate" }
      }
    ]
  }